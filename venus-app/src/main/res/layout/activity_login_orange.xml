<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/container"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  tools:context="com.emis.venus.activity.LoginActivity"
  tools:layout_editor_absoluteY="25dp">

  <View
    android:id="@+id/view2"
    android:layout_width="wrap_content"
    android:layout_height="50dp"
    android:background="@drawable/date_frame"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"/>

  <TextView
    android:id="@+id/txtDate"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="16dp"
    android:text="TextView"
    android:textColor="#FFFFFF"
    android:textSize="20sp"
    app:layout_constraintBottom_toBottomOf="@+id/view2"
    app:layout_constraintStart_toStartOf="@+id/view2"
    app:layout_constraintTop_toTopOf="parent"/>

  <View
    android:id="@+id/view6"
    android:layout_width="300dp"
    android:layout_height="50dp"
    android:layout_marginTop="20dp"
    android:background="@drawable/login_input_frame"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/view2"/>

  <View
    android:id="@+id/view7"
    android:layout_width="300dp"
    android:layout_height="50dp"
    android:background="@drawable/login_input_frame"
    app:layout_constraintEnd_toEndOf="@+id/view6"
    app:layout_constraintHorizontal_bias="0.495"
    app:layout_constraintStart_toStartOf="@+id/view6"
    app:layout_constraintTop_toBottomOf="@+id/view6"/>

  <View
    android:id="@+id/view8"
    android:layout_width="300dp"
    android:layout_height="50dp"
    android:background="@drawable/login_input_frame"
    app:layout_constraintEnd_toEndOf="@+id/view6"
    app:layout_constraintHorizontal_bias="1.0"
    app:layout_constraintStart_toStartOf="@+id/view6"
    app:layout_constraintTop_toBottomOf="@+id/view7"/>

  <View
    android:id="@+id/view9"
    android:layout_width="300dp"
    android:layout_height="50dp"
    android:background="@drawable/login_input_frame"
    app:layout_constraintEnd_toEndOf="@+id/view6"
    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintStart_toStartOf="@+id/view6"
    app:layout_constraintTop_toBottomOf="@+id/view8"/>

  <TextView
    android:id="@+id/textView25"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="20dp"
    android:layout_marginTop="5dp"
    android:layout_marginBottom="6dp"
    android:text="账号"
    android:textColor="@color/black"
    app:layout_constraintBottom_toBottomOf="@+id/view6"
    app:layout_constraintStart_toStartOf="@+id/view6"
    app:layout_constraintTop_toTopOf="@+id/view6"/>

  <EditText
    android:id="@+id/edtUserID"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:ems="10"
    android:hint="使用者名称"
    android:inputType="textPersonName"
    app:layout_constraintBottom_toBottomOf="@+id/textView25"
    app:layout_constraintEnd_toEndOf="@+id/view6"
    app:layout_constraintStart_toEndOf="@+id/textView25"
    app:layout_constraintTop_toTopOf="@+id/textView25"/>

  <TextView
    android:id="@+id/textView26"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="17dp"
    android:layout_marginBottom="14dp"
    android:text="密码"
    android:textColor="@color/black"
    app:layout_constraintBottom_toBottomOf="@+id/view7"
    app:layout_constraintEnd_toEndOf="@+id/textView25"
    app:layout_constraintStart_toStartOf="@+id/textView25"
    app:layout_constraintTop_toTopOf="@+id/view7"/>

  <TextView
    android:id="@+id/textView28"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="15dp"
    android:layout_marginBottom="16dp"
    android:text="门市"
    android:textColor="@color/black"
    app:layout_constraintBottom_toBottomOf="@+id/view8"
    app:layout_constraintEnd_toEndOf="@+id/textView25"
    app:layout_constraintStart_toStartOf="@+id/textView25"
    app:layout_constraintTop_toTopOf="@+id/view8"/>

  <TextView
    android:id="@+id/textView29"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="13dp"
    android:layout_marginBottom="18dp"
    android:text="机号"
    android:textColor="@color/black"
    app:layout_constraintBottom_toBottomOf="@+id/view9"
    app:layout_constraintEnd_toEndOf="@+id/textView25"
    app:layout_constraintStart_toStartOf="@+id/textView25"
    app:layout_constraintTop_toTopOf="@+id/view9"/>

  <EditText
    android:id="@+id/edtPassword"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:ems="10"
    android:hint="请输入密码"
    android:inputType="textPassword"
    android:selectAllOnFocus="true"
    app:layout_constraintBottom_toBottomOf="@+id/view7"
    app:layout_constraintEnd_toEndOf="@+id/view7"
    app:layout_constraintStart_toEndOf="@+id/textView26"
    app:layout_constraintTop_toTopOf="@+id/view7"/>

  <EditText
    android:id="@+id/edtStore"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:ems="10"
    android:hint="门市号码"
    android:inputType="textPersonName"
    app:layout_constraintBottom_toBottomOf="@+id/view8"
    app:layout_constraintEnd_toEndOf="@+id/view8"
    app:layout_constraintStart_toEndOf="@+id/textView28"
    app:layout_constraintTop_toTopOf="@+id/view8"/>

  <EditText
    android:id="@+id/edtCashRegister"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:ems="10"
    android:hint="机台号码"
    android:inputType="textPersonName"
    app:layout_constraintBottom_toBottomOf="@+id/view9"
    app:layout_constraintEnd_toEndOf="@+id/view9"
    app:layout_constraintStart_toEndOf="@+id/textView29"
    app:layout_constraintTop_toTopOf="@+id/view9"/>

  <Button
    android:id="@+id/btnLogin"
    android:layout_width="300dp"
    android:layout_height="50dp"
    android:layout_marginTop="30dp"
    android:background="@drawable/login_button"
    android:onClick="btnLoginClick"
    android:text="登入"
    android:textColor="@color/antiquewhite"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/view9"/>

  <Button
    android:id="@+id/btnGetData"
    android:layout_width="300dp"
    android:layout_height="50dp"
    android:layout_marginTop="16dp"
    android:background="@drawable/login_button"
    android:onClick="btnGetDataClick"
    android:text="基本资讯载入"
    android:textColor="@color/antiquewhite"
    app:layout_constraintEnd_toEndOf="@+id/btnLogin"
    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintStart_toStartOf="@+id/btnLogin"
    app:layout_constraintTop_toBottomOf="@+id/btnReset"/>

  <Button
    android:id="@+id/btnReset"
    android:layout_width="300dp"
    android:layout_height="50dp"
    android:layout_marginTop="16dp"
    android:background="@drawable/login_button"
    android:onClick="btnResetClick"
    android:text="清除"
    android:textColor="@color/antiquewhite"
    app:layout_constraintEnd_toEndOf="@+id/btnLogin"
    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintStart_toStartOf="@+id/btnLogin"
    app:layout_constraintTop_toBottomOf="@+id/btnLogin"/>

  <TextView
    android:id="@+id/txtDay"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="10dp"
    android:text="TextView"
    android:textColor="#FFFFFF"
    app:layout_constraintBottom_toBottomOf="@+id/txtDate"
    app:layout_constraintStart_toEndOf="@+id/txtDate"
    app:layout_constraintTop_toTopOf="@+id/txtDate"/>

  <TextView
    android:id="@+id/txtMsg"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    app:layout_constraintBottom_toTopOf="@+id/btnLogin"
    app:layout_constraintTop_toBottomOf="@+id/edtCashRegister"
    tools:layout_editor_absoluteX="54dp"/>

  <android.inputmethodservice.KeyboardView
    android:id="@+id/keyboard"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/keyboard_base"
    android:foregroundGravity="center_horizontal"
    android:keyBackground="@drawable/keyboard_button1"
    android:paddingTop="5dp"
    android:paddingBottom="5dp"
    android:visibility="invisible"
    app:layout_constraintBottom_toBottomOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>