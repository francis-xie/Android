<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/constraintlayout"
  android:layout_width="match_parent"
  android:layout_height="match_parent">

  <EditText
    android:id="@+id/edtID_NO"
    android:layout_width="168dp"
    android:layout_height="48dp"
    android:layout_marginStart="8dp"
    android:clickable="false"
    android:ems="10"
    android:focusable="false"
    android:focusableInTouchMode="false"
    android:hint="机台编号"
    android:inputType="textPersonName"
    android:textColor="@color/black"
    app:layout_constraintBaseline_toBaselineOf="@+id/txtID_NO"
    app:layout_constraintStart_toEndOf="@+id/txtID_NO"/>

  <TextView
    android:id="@+id/txtID_NO"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="32dp"
    android:text="机台编号"
    android:textColor="@color/black"
    android:textSize="20sp"
    app:layout_constraintEnd_toStartOf="@+id/edtSL_NO"
    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintStart_toStartOf="@+id/txtSL_NO"
    app:layout_constraintTop_toBottomOf="@+id/txtSL_NO"/>

  <EditText
    android:id="@+id/edtSL_NO"
    android:layout_width="168dp"
    android:layout_height="48dp"
    android:layout_marginStart="8dp"
    android:clickable="false"
    android:ems="10"
    android:focusable="false"
    android:focusableInTouchMode="false"
    android:hint="门市名称"
    android:inputType="textPersonName"
    android:textColor="@color/black"
    app:layout_constraintBaseline_toBaselineOf="@+id/txtSL_NO"
    app:layout_constraintStart_toEndOf="@+id/txtSL_NO"/>

  <TextView
    android:id="@+id/txtSL_NO"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="32dp"
    android:text="门市名称"
    android:textColor="@color/black"
    android:textSize="20sp"
    app:layout_constraintEnd_toEndOf="@+id/txtINV_SELECT"
    app:layout_constraintStart_toStartOf="@+id/txtINV_SELECT"
    app:layout_constraintTop_toBottomOf="@+id/txtINV_SELECT"/>

  <EditText
    android:id="@+id/edtINV_SELECT"
    android:layout_width="168dp"
    android:layout_height="48dp"
    android:layout_marginStart="8dp"
    android:clickable="false"
    android:ems="10"
    android:focusable="false"
    android:focusableInTouchMode="false"
    android:hint="门市编号"
    android:inputType="textPersonName"
    android:textColor="@color/black"
    app:layout_constraintBaseline_toBaselineOf="@+id/txtINV_SELECT"
    app:layout_constraintStart_toEndOf="@+id/txtINV_SELECT"/>

  <TextView
    android:id="@+id/txtINV_SELECT"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="32dp"
    android:layout_marginTop="32dp"
    android:text="门市编号"
    android:textColor="@color/black"
    android:textSize="20sp"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>
